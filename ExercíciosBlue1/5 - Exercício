// Exercício 5 - Elabore um programa que receba 3 notas de um aluno e calcule a média dele.

// Exemplo:
// Primeira nota = 7
// Segunda nota = 8
// Terceira nota = 10
let n1 = parseFloat(prompt('Insira a primeira nota:'));
let n2 = parseFloat(prompt('Insira a segunda nota:'));
let n3 = parseFloat(prompt('Insira a terceira nota:'));
var DECIMAL = 10;
console.log('A média é:');
console.log((n1+n2+n3)/3);
